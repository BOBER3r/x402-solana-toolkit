{
  "name": "@x402-solana/react",
  "version": "0.3.1",
  "description": "React hooks and UI components for x402 payment protocol on Solana",
  "main": "dist/index.js",
  "module": "dist/index.mjs",
  "types": "dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "require": "./dist/index.js",
      "import": "./dist/index.mjs"
    }
  },
  "scripts": {
    "build": "tsup",
    "dev": "tsup --watch",
    "test": "jest",
    "clean": "rm -rf dist",
    "prepublishOnly": "npm run clean && npm run build"
  },
  "keywords": [
    "solana",
    "x402",
    "react",
    "hooks",
    "components",
    "ui",
    "wallet",
    "payment",
    "usdc",
    "web3"
  ],
  "author": "x402-solana-toolkit contributors",
  "license": "MIT",
  "peerDependencies": {
    "@solana/wallet-adapter-base": "^0.9.23",
    "@solana/wallet-adapter-react": "^0.15.35",
    "@solana/web3.js": "^1.95.0",
    "react": "^18.0.0"
  },
  "dependencies": {
    "@solana/spl-token": "^0.3.11",
    "@x402-solana/client": "^0.2.0",
    "@x402-solana/core": "^0.2.0"
  },
  "devDependencies": {
    "@solana/wallet-adapter-base": "^0.9.27",
    "@solana/wallet-adapter-react": "^0.15.39",
    "@testing-library/react": "^14.1.2",
    "@types/jest": "^29.5.11",
    "@types/node": "^20.10.6",
    "@types/react": "^18.2.46",
    "jest": "^29.7.0",
    "jest-environment-jsdom": "^29.7.0",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "ts-jest": "^29.1.1",
    "tsup": "^8.5.0",
    "typescript": "^5.3.3"
  },
  "files": [
    "dist",
    "README.md"
  ],
  "publishConfig": {
    "access": "public"
  }
}
